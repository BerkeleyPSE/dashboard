  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" --kill-others-on-fail",
    "build": "yarn run build --prefix client",
    "heroku-postbuild": "yarn install --prefix client && yarn run build --prefix client"
  },
  "devDependencies": {
    "concurrently": "3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "nodemon": "^1.14.12"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "express": "4.16.1",
    "lodash": "^4.17.5",
    "mongoose": "5.0.4",
    "passport": "^0.4.0"
  }
}
